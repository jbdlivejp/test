<struts>
	<constant name="struts.devMode" value="true"/>
	<package nam="com.internousdev.ecsite3.action"extends="struts-default">

		<action name="GoHomeActuion" class="com.internousdev.ecsite3.GoHomeAction" method="execute">
			<result name="success">home.jsp</result>
		</action>

		<action name="HomeActuion" class="com.internousdev.ecsite3.HomeAction" method="execute">
			<result name="login">login.jsp</result>
			<result name="success">buyItem.jsp</result>
		</action>

		<action name="LoginActuion" class="com.internousdev.ecsite3.LoginAction" method="execute">
			<result name="success">buyItem.jsp</result>
			<result name="error">logiin.jsp</result>
		</action

		<action name="UserCreateActuion" class="com.internousdev.ecsite3.UserCteateAction" method="execute">
			<result name="success">userCreate.jsp</result>
			<result name="back">logiin.jsp</result>

			<action name="UserCreateConfirmActuion" class="com.internousdev.ecsite3.UserCreateConfirmAction" method="execute">
			<result name="success">userCreateConfirm.jsp</result>
			<result name="error">userCreate.jsp</result>
		</action
		</action

	</package>
</struts>